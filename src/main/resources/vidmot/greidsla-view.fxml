<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #fafafa;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vidmot.GreidslaController">
   <Label fx:id="fxLabel" alignment="CENTER" prefHeight="65.0" prefWidth="560.0" style="-fx-background-color: #e3dbd9#e3dbd9;">
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin>
   </Label>
   <HBox prefHeight="60.0" prefWidth="360.0">
      <children>
         <Button mnemonicParsing="false" onAction="#fxHaettaVidButton" prefHeight="58.0" prefWidth="180.0" style="-fx-background-color: #89b4c4;" text="Hætta við?" />
         <Button fx:id="fxStadfestaButton" mnemonicParsing="false" onAction="#fxStadfestaButton" prefHeight="58.0" prefWidth="180.0" style="-fx-background-color: #89b4c4;" text="Staðfesta">
            <HBox.margin>
               <Insets left="10.0" right="10.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="fxGreidaButton" mnemonicParsing="false" onAction="#fxGreidaButton" disable="true" prefHeight="58.0" prefWidth="180.0" style="-fx-background-color: #89b4c4;" text="Greiða">
            <HBox.margin>
               <Insets />
            </HBox.margin></Button>
      </children>
   </HBox>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>

</VBox>
